.container{
    background: #f5f5f7;
    min-height: 1254rpx;
    display: block;
}

.prompt{
    font-size: 30rpx;
    line-height: 78rpx;
    text-align: left;
    color: #808080;
    padding-left:72rpx;
    width:100%;
}

.prompt-img{
    position: absolute;
    width: 30rpx;
    height:30rpx;
    left:32rpx;
    top:24rpx;
    display: block;
}

.block{
    width:100%;
    height: 98rpx;
    background: white;
}

.login{
  width:516rpx;
  height:88rpx;
  background: #FF9354;
  border-radius: 5rpx;
  line-height: 88rpx;
  color: white;
  font-size: 32rpx;
  text-align:center;
}

.input{
  float: right;
  width:488rpx;
  height: 98rpx;
  color:#999999;
  font-size:34rpx;
  line-height: 98rpx;
  border-bottom: 1rpx solid #DBDBDB;
}

.text{
  color:#666666;
}

.label{
  float: right;
  width:230rpx;
  height: 98rpx;
  color:#666666;
  font-size:34rpx;
  line-height: 98rpx;
  border-bottom: 1rpx solid #DBDBDB;
}



